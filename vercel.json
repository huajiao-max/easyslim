{
  "version": 2,
  "builds": [
    {
      "src": ".output/server/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "Cache-Control": "public,max-age=31536000,immutable"
      }
    },
    {
      "src": "/(.*)", 
      "dest": ".output/server/index.mjs"
    }
  ]
}
