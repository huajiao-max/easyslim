{
    "version": 2,
    "builds": [
      {
         "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "serverFiles": ["server-middleware/**"]
      }
    ],
     "nodeVersion": "16.20.0",
    "routes": [
      { "src": "/_nuxt/.+", "headers": { "Cache-Control": "public,max-age=31536000,immutable" } },
    { "src": "/(.*)", "dest": "/" }
    ],
    "functions": {
    "api/**": {
   
      "excludeFiles": "{.nuxt,*.cache,node_modules,public,app}/**"
    }}
  }
