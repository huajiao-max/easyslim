{
  "name": "month-plan-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi preview",
    "generate": "nuxi generate",
    "test": "jest"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es2021": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2021,
      "sourceType": "module",
      "requireConfigFile": false
    },
    "extends": [
      "plugin:vue/recommended",
      "plugin:nuxt/recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-v-for-template-key": "off"
    }
  },
  "dependencies": {
    "@element-plus/nuxt": "^1.1.3",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/robots": "^5.2.10",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "core-js": "^3.25.5",
    "cos-js-sdk-v5": "^1.10.1",
    "element-plus": "^2.10.1",
    "firebase": "^9.15.0",
    "nuxt": "^3.17.5",
    "pinia": "^3.0.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "eslint-plugin-nuxt": "^4.0.0",
    "esm": "^3.2.25",
    "less": "^4.3.0"
  },
  "overrides": {
    "defu": "6.1.4"
  }
}
